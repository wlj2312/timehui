<view class="content"><view class="row b-b center"><text class="tit">联系人</text><input class="input" type="text" placeholder="姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="row b-b center"><text class="tit">电话</text><input class="input" type="number" maxlength="11" placeholder="手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['','mobile','$event',[]]]]]]}}" value="{{mobile}}" bindinput="__e"/></view><view class="row b-b flex-left"><text class="tit">地址</text><block wx:if="{{planLocate=='请选择地址'}}"><address-picker style="font-size:30rpx;color:grey;" vue-id="d1f06a76-1" level="{{3}}" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}">请选择地址</address-picker></block><block wx:if="{{planLocate!='请选择地址'}}"><address-picker style="font-size:30rpx;color:#333333;" vue-id="d1f06a76-2" level="{{3}}" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}">{{planLocate+''}}</address-picker></block></view><view class="row b-b"><input class="input" type="text" placeholder-style="font-size: 30rpx;color: grey;" placeholder="详细地址" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['','street','$event',[]]]]]]}}" value="{{street}}" bindinput="__e"/></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn flex-center" bindtap="__e">确认添加</button></view>