page {
  padding-top: 16rpx;
}
.center {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.row {
  position: relative;
  padding: 0 30rpx;
  height: 110rpx;
  background: #fff;
  font-size: 30rpx;
}
.row .tit {
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  width: 120rpx;
  font-size: 30rpx;
  margin-right: 10rpx;
}
.row .input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  font-size: 30rpx;
}
.row .iconenter {
  font-size: 36rpx;
  color: #cccccc;
}
.default-row {
  margin-top: 16rpx;
}
.default-row .tit {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.default-row switch {
  -webkit-transform: translateX(16rpx) scale(0.9);
          transform: translateX(16rpx) scale(0.9);
}
.delete {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
  padding: 0 30rpx;
  height: 110rpx;
  background: #fff;
  margin-top: 28rpx;
  font-size: 30rpx;
  font-weight: 500;
  color: #fa2740;
}
.add-btn {
  margin: 0 14rpx;
  background: -webkit-gradient(linear, left top, right top, from(#79D4BD), color-stop(99%, #099999));
  background: -webkit-linear-gradient(left, #79D4BD 0%, #099999 99%);
  background: linear-gradient(90deg, #79D4BD 0%, #099999 99%);
  border-radius: 10rpx;
  color: #fff;
}
.alert-box {
  background-color: #ffffff;
}
.margin-t-20 {
  margin-top: 20rpx !important;
}

